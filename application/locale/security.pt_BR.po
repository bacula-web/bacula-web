msgid ""
msgstr ""
"Content-Type: text/plain; charset=UTF-8\n"
"Content-Transfer-Encoding: 8bit\n"
"Language: pt_BR\n"

msgid "An authentication exception occurred."
msgstr "Uma exceção ocorreu durante a autenticação."

msgid "Authentication credentials could not be found."
msgstr "As credenciais de autenticação não foram encontradas."

msgid "Authentication request could not be processed due to a system problem."
msgstr "A solicitação de autenticação não pôde ser processada devido a um problema no sistema."

msgid "Invalid credentials."
msgstr "Credenciais inválidas."

msgid "Cookie has already been used by someone else."
msgstr "Este cookie já foi usado por outra pessoa."

msgid "Not privileged to request the resource."
msgstr "Sem privilégio para solicitar o recurso."

msgid "Invalid CSRF token."
msgstr "Token CSRF inválido."

msgid "No authentication provider found to support the authentication token."
msgstr "Nenhum provedor de autenticação encontrado para suportar o token de autenticação."

msgid "No session available, it either timed out or cookies are not enabled."
msgstr "Nenhuma sessão disponível, ela expirou ou os cookies não estão habilitados."

msgid "No token could be found."
msgstr "Nenhum token foi encontrado."

msgid "Username could not be found."
msgstr "Nome de usuário não encontrado."

msgid "Account has expired."
msgstr "A conta está expirada."

msgid "Credentials have expired."
msgstr "As credenciais estão expiradas."

msgid "Account is disabled."
msgstr "Conta desativada."

msgid "Account is locked."
msgstr "A conta está travada."

msgid "Too many failed login attempts, please try again later."
msgstr "Muitas tentativas de login malsucedidas, tente novamente mais tarde."

msgid "Invalid or expired login link."
msgstr "Link de login inválido ou expirado."

msgid "Too many failed login attempts, please try again in %minutes% minute."
msgstr "Muitas tentativas de login inválidas, por favor, tente novamente em um minuto."

msgid "Too many failed login attempts, please try again in %minutes% minutes."
msgstr "Muitas tentativas de login sem sucesso, por favor tente novamente em %minutes% minutos."
