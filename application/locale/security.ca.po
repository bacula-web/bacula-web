msgstr ""
"Content-Type: text/plain; charset=UTF-8\n"
"Content-Transfer-Encoding: 8bit\n"
"Language: ca\n"

msgid "An authentication exception occurred."
msgstr "Ha succeït un error d'autenticació."

msgid "Authentication credentials could not be found."
msgstr "No s'han trobat les credencials d'autenticació."

msgid "Authentication request could not be processed due to a system problem."
msgstr "La solicitud d'autenticació no s'ha pogut processar per un problema del sistema."

msgid "Invalid credentials."
msgstr "Credencials no vàlides."

msgid "Cookie has already been used by someone else."
msgstr "La cookie ja ha estat utilitzada per una altra persona."

msgid "Not privileged to request the resource."
msgstr "No té privilegis per solicitar el recurs."

msgid "Invalid CSRF token."
msgstr "Token CSRF no vàlid."

msgid "No authentication provider found to support the authentication token."
msgstr "No s'ha trobat un proveïdor d'autenticació que suporti el token d'autenticació."

msgid "No session available, it either timed out or cookies are not enabled."
msgstr "No hi ha sessió disponible, ha expirat o les cookies no estan habilitades."

msgid "No token could be found."
msgstr "No s'ha trobat cap token."

msgid "Username could not be found."
msgstr "No s'ha trobat el nom d'usuari."

msgid "Account has expired."
msgstr "El compte ha expirat."

msgid "Credentials have expired."
msgstr "Les credencials han expirat."

msgid "Account is disabled."
msgstr "El compte està deshabilitat."

msgid "Account is locked."
msgstr "El compte està bloquejat."

msgid "Too many failed login attempts, please try again later."
msgstr "Massa intents d'inici de sessió fallits, torneu-ho a provar més tard."

msgid "Invalid or expired login link."
msgstr "Enllaç d'inici de sessió no vàlid o caducat."

msgid "Too many failed login attempts, please try again in %minutes% minute."
msgstr "Massa intents d'inici de sessió fallits, torneu-ho a provar en %minutes% minut."

msgid "Too many failed login attempts, please try again in %minutes% minutes."
msgstr "Massa intents fallits d'inici de sessió, torneu-ho a provar d'aquí a %minutes% minuts."
